<body>
    <section class="body">

        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a href="/" class="logo">
                    <img src="~/Images/BACKGROUND/logo.png" height="45" alt="Porto Admin" />
                </a>

                <div class="d-md-none toggle-menu" data-bs-toggle="collapse" data-bs-target=".tab-navigation">
                    <i class="fas fa-bars" aria-label="Toggle Menu"></i>
                </div>

            </div>

            <!-- start: search & user box -->
            <div class="header-right">
                <span class="separator"></span>

                <ul class="notifications">
                    <li>
                        <a href="#" class="dropdown-toggle notification-icon" data-bs-toggle="dropdown">
                            <i class="bx bx-envelope"></i>
                            <span class="badge"></span>
                        </a>

                        <div class="dropdown-menu notification-menu">
                            <div class="notification-title">
                                <span class="float-end badge badge-default">0</span>
                                Menssagens
                            </div>

                            <div class="content">
                                <ul>
                                    <li>
                                        <a href="#" class="clearfix">
                                            <div class="image">
                                                <i class="el el-exclamation-sign bg-dark text-light"></i>
                                            </div>
                                            <span class="title">Caixa de menssagens</span>
                                            <span class="message"> vazia.</span>
                                        </a>
                                    </li>
                                </ul>

                                <hr />

                                <div class="text-end">
                                    <a href="#" class="view-more"></a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle notification-icon" data-bs-toggle="dropdown">
                            <i class="bx bx-bell"></i>
                            <span class="badge"></span>
                        </a>

                        <div class="dropdown-menu notification-menu">
                            <div class="notification-title">
                                <span class="float-end badge badge-default">0</span>
                                Notificações
                            </div>

                            <div class="content">
                                <ul>
                                    <li>
                                        <a href="#" class="clearfix">
                                            <div class="image">
                                                <i class="el el-exclamation-sign bg-dark text-light"></i>
                                            </div>
                                            <span class="title">Caixa de notificação</span>
                                            <span class="message"> vazia.</span>
                                        </a>
                                    </li>
                                </ul>

                                <hr />

                                <div class="text-end">
                                    <a href="#" class="view-more"></a>
                                </div>
                            </div>
                        </div>
                    </li>
                </ul>

                <span class="separator"></span>

                <div id="userbox" class="userbox">
                    <a href="#" data-bs-toggle="dropdown">
                        <div class="profile-info" data-lock-name="John Doe" data-lock-email="johndoe@okler.com">
                            <span class="name">@ViewBag.NomeUsuario</span>
                            <span class="role">@ViewBag.Permissao</span>
                        </div>

                        <i class="fa custom-caret"></i>
                    </a>

                    <div class="dropdown-menu">
                        <ul class="list-unstyled mb-2">
                            <li class="divider"></li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="/Acesso/Login"><i class="bx bx-power-off"></i> Sair</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>

        <div class="inner-wrapper">
            <div class="tab-navigation collapse">
                <nav>
                    <ul class="nav nav-pills">
                        <li class="nav-expanded nav-active">
