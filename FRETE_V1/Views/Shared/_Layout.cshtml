@{
    if (TempData["Msg"] != null)
    {
        <script>
                (function ($) {
                    'use strict';
                    var stack_bar_bottom = { "dir1": "up", "dir2": "right", "spacing1": 0, "spacing2": 0 };
                    var notice = new PNotify({
                        title: 'Notificação',
                        text: '@TempData["Msg"]',
                        addclass: 'notification-primary stack-bar-bottom',
                        icon: 'el el-exclamation-sign',
                        stack: stack_bar_bottom,
                        width: "70%"
                    });
                }).apply(this, [jQuery]);
        </script>
    }
}