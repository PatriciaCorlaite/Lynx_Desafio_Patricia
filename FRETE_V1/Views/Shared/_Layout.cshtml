                                <i class="el el-calendar" aria-hidden="true"></i> Agenda
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
            <section role="main" class="content-body">
                <!-- start: page -->
                @Scripts.Render("~/bundles/jquery")
                @RenderSection("featured", required: false)
                @RenderBody()
                <!-- end: page -->
            </section>
        </div>

        <aside id="sidebar-right" class="sidebar-right">
            <div class="nano">
